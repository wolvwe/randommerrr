-- Scripts

local function SOQS_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)

	script.Parent.MouseButton1Down:Connect(function()
		local a = Instance.new("Sound", workspace)
		a.SoundId = "rbxassetid://95156028272944"
		a.Pitch = 0.2
		a.Volume = 3
		a:Play()
	end)
end
coroutine.wrap(SOQS_fake_script)()
local function EVTZQ_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	script.Parent.MouseButton1Down:Connect(function()
		s = Instance.new("Sky")
		s.Name = "Sky"
		s.Parent = game.Lighting
		s.SkyboxBk = "http://www.roblox.com/asset/?id=10180536577"
		s.SkyboxDn = "http://www.roblox.com/asset/?id=10180536577"
		s.SkyboxFt = "http://www.roblox.com/asset/?id=10180536577"
		s.SkyboxLf = "http://www.roblox.com/asset/?id=10180536577"
		s.SkyboxRt = "http://www.roblox.com/asset/?id=10180536577"
		s.SkyboxUp = "http://www.roblox.com/asset/?id=10180536577"
	end)
end
