local function LIKBCTR_fake_script() -- TextButton_8.Script 
	local script = Instance.new('Script', TextButton_8)

	script.Parent.MouseButton1Click:Connect(function()
		while true do
			wait(0.5)
			for i, v in pairs(game.Players:GetChildren()) do
				pcall(function()
					local taco = Instance.new("Part", game.Workspace)
					taco.CanCollide = true
					local m = Instance.new("SpecialMesh", taco)
					m.MeshId = "http://www.roblox.com/asset/?id=124900173701701"
					m.TextureId = "http://www.roblox.com/asset/?id=102923004202436"
					taco.CFrame = v.Character.Head.CFrame + Vector3.new(math.random(-50,50),100,math.random(-50,50))
				end)
			end
		end
	end)
	
end
coroutine.wrap(LIKBCTR_fake_script)()
