local function POBA_fake_script() -- TextButton_6.Script 
	local script = Instance.new('Script', TextButton_6)

	script.Parent.MouseButton1Down:Connect(function()
		for _, part in pairs(workspace:GetDescendants()) do
			if part:IsA("BasePart") then
				local particle = Instance.new("ParticleEmitter")
				particle.Parent = part
				particle.SpreadAngle = Vector2.new(0, 0)
				particle.Rate = 50
				particle.Speed = NumberRange.new(10)
				particle.Texture = "rbxassetid://10180536577"
			end
		end
	end)
end
coroutine.wrap(POBA_fake_script)()
